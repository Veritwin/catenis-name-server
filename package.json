{
  "name": "catenis-name-server",
  "version": "1.0.0",
  "description": "Implementation of Catenis Name Service",
  "main": "build/main.js",
  "scripts": {
    "cleanup-build": "rm -rf build/*",
    "cleanup-dist": "rm -f dist/$npm_package_name-$npm_package_version.tgz",
    "cleanup-dist-dev": "rm -f dist/dev/$npm_package_name-$npm_package_version.tgz",
    "cleanup": "npm run cleanup-build && npm run cleanup-dist",
    "cleanup-dev": "npm run cleanup-build && npm run cleanup-dist-dev",
    "build": "npm run cleanup-build && babel src -d build",
    "predeploy": "npm run cleanup && npm run build && npm pack && mv *.tgz dist/",
    "predeploy-dev": "bin/npmignore_dev.sh && npm run cleanup-dev && npm run build && npm pack && mv *.tgz dist/dev && bin/npmignore_dev.sh reset",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Catenis",
    "Catenis Name Service"
  ],
  "author": "Cl√°udio de Castro",
  "license": "MIT",
  "bin": {
    "ctnnamesrv": "./build/main.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0"
  },
  "dependencies": {
    "async": "^3.1.0",
    "config": "^3.2.2",
    "core-js": "^2.6.9",
    "fibers": "^4.0.1",
    "http-signature": "^1.2.0",
    "lokijs": "^1.5.7",
    "moment": "^2.24.0",
    "restify": "^8.4.0",
    "restify-clients": "^2.6.7",
    "restify-errors": "^8.0.1",
    "triple-beam": "^1.3.0",
    "underscore": "^1.9.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.1.0",
    "winston-mail": "^2.0.0"
  }
}
