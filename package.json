{
  "name": "catenis-off-chain-server",
  "version": "1.1.0",
  "description": "REST API service for sending and receiving Catenis off-chain messages",
  "main": "build/main.js",
  "scripts": {
    "cleanup-build": "rm -rf build/*",
    "cleanup-dist": "rm -f dist/$npm_package_name-$npm_package_version.tgz",
    "cleanup-dist-dev": "rm -f dist/dev/$npm_package_name-$npm_package_version.tgz",
    "cleanup": "npm run cleanup-build && npm run cleanup-dist",
    "cleanup-dev": "npm run cleanup-build && npm run cleanup-dist-dev",
    "build": "npm run cleanup-build && babel src -d build && chmod u+x build/main.js",
    "predeploy": "npm run cleanup && npm run build && npm pack && mv *.tgz dist/",
    "predeploy-dev": "bin/npmignore_dev.sh && npm run cleanup-dev && npm run build && npm pack && mv *.tgz dist/dev && bin/npmignore_dev.sh reset",
    "test": "env NODE_APP_INSTANCE=test mocha --require @babel/polyfill --require @babel/register"
  },
  "keywords": [
    "Catenis",
    "off-chain",
    "server"
  ],
  "author": "Cl√°udio de Castro",
  "private": true,
  "bin": {
    "ctnocsvr": "./build/main.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "chai": "^4.2.0",
    "mocha": "^6.2.2"
  },
  "dependencies": {
    "async": "^3.1.0",
    "catenis-off-chain-lib": "git+ssh://git@bitbucket.org/blockchainofthings/catenis-off-chain-lib.git#semver:^1.0.0",
    "config": "^3.2.4",
    "core-js": "^3.4.1",
    "fibers": "^4.0.2",
    "http-signature": "^1.3.1",
    "ipfs-http-client": "^44.0.3",
    "it-to-stream": "^0.1.1",
    "moment": "^2.24.0",
    "mongodb": "^3.3.4",
    "restify": "^8.4.0",
    "restify-clients": "^2.6.7",
    "restify-errors": "^8.0.1",
    "triple-beam": "^1.3.0",
    "underscore": "^1.9.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.2.1",
    "winston-mail": "^2.0.0",
    "ws": "^7.2.0"
  },
  "repository": {
    "directory": "git@bitbucket.org:blockchainofthings/catenis-off-chain-server.git"
  }
}
